class Solution:
    def bubbleSort(self, nums):
        n=len(nums)
        for i in range(n-1):
            didswap=0
            for j in range(n-1-i):
                if nums[j]>nums[j+1]:
                    nums[j],nums[j+1]=nums[j+1],nums[j]
                    didswap=1
            if didswap==0:
                break
        return nums
