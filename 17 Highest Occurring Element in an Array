class Solution:
    def mostFrequentElement(self, nums):
        freq={}
        for x in nums:
            freq[x]=freq.get(x,0)+1
        max_freq=0
        ans=float('inf')
        for k,v in freq.items():
            if v>max_freq or (v==max_freq and k<ans):
                max_freq=v
                ans=k
        return ans
