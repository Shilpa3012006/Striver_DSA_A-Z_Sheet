from math import sqrt
class Solution:
    def isPrime(self, n):
        #your code goes here
        div=[]
        for i in range(1,int(sqrt(n))+1):
            if n%i==0:
                div.append(i)
                if n/i!=i:
                    div.append(n/i)
            if len(div)>2:
                return False
        return True
