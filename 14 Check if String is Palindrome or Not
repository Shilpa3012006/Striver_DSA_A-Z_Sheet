class Solution:    
    def palindromeCheck(self, s):
        #your code goes here
        def is_polindrome(l,r):
            if l>=r:
                return True
            if s[l]!=s[r]:
                return False
            return is_polindrome(l+1,r-1)
        return is_polindrome(0,len(s)-1)

            
       
