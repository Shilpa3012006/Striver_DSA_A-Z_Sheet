# 1st method

class Solution:
    def countDigit(self, n):
        count=0 
        while n>0:
            last_digit=n%10
            count=count+1
            n=n//10
        return count

#2nd method
from math import log10
class Solution:
    def countDigit(self, n):
        return int(log10(n)+1)
